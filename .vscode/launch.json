    "version": "0.2.0",
    "configurations": [
    {
        "name": "Generate Documentation",
        "program": "${workspaceFolder}/docs/gen_cmds.js",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "pwa-node"
    },

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Start Bot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": ["--trace-warnings"]
        }
    ]
}